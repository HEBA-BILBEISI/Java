<body>
   <script>
function stopwatch (){
let startTime,endTime , running , duraiton = 0;

this.start = function (){
 if(running)
 throw new Error ('stopwatch has already started.')
 running = true ;
 startTime = new Date ();
};

this.stop= function (){
if (!running)
throw new Error ('stopwatch is not started.');
running : false ;
endTime = new Date ();

const seconds = (endTime.getTime() - startTime.getTime())/1000;
duraiton+= seconds;

};

this.reset= function (){
    startTime = null;
    endTime = null;
    running = false;
    duraiton = 0 ;
};

Object.defineProperty(this , 'duration',{
get : function(){ return duraiton ;}
});
}







   </script> 

</body>
</html>


